<div id='catalog'>
	<div class='owl2'>
	<% @products.each do |product| %>
		<% url = spree.product_url(product) %>
		<div>
			<%= link_to url do %>
				<h3> <%= product.name %> </h3>
				<% if defined? product.master_images.first %>
					<%= image_tag product.master_images.first.attachment.url %>
				<% end %>
			<% end %>

		</div>
	<% end %>
	</div>
</div>

<div class='owl-controls theNav'>
</div>
