<% @products.each do |product| %>
	<% url = spree.product_url(product) %>
	<div class='product-teaser'>
		<%= link_to url do %>
			<h4> <%= product.name %> </h4>
			<% if product.master_images.count > 0 %>
				<%= image_tag product.master_images.first.attachment.url %>
			<% end %>
		<% end %>
	</div>
<% end %>
